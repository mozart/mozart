        echo "$ac_t""yes" 1>&6
    else
        echo "$ac_t""no" 1>&6
        cat >> confdefs.h <<\EOF
#define NOFINALIZATION 1
EOF


    fi




    echo $ac_n "checking for --enable-foreign-pointer""... $ac_c" 1>&6
echo "configure:2724: checking for --enable-foreign-pointer" >&5
    # Check whether --enable-foreign-pointer or --disable-foreign-pointer was given.
if test "${enable_foreign_pointer+set}" = set; then
  enableval="$enable_foreign_pointer"
  :
fi

    if test -n "${enable_foreign_pointer}"; then
        oz_enable_foreign_pointer=$enable_foreign_pointer
    fi
    if test "${oz_enable_foreign_pointer}" != no; then
        echo "$ac_t""yes" 1>&6
    else
        echo "$ac_t""no" 1>&6
        cat >> confdefs.h <<\EOF
#define NOFOREIGN_POINTER 1
EOF


    fi





